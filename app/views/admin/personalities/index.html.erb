<p id="notice"><%= notice %></p>

<h1>Personalities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personalities.each do |personality| %>
      <tr>
        <td><%= link_to 'Show', personality %></td>
        <td><%= link_to 'Edit', edit_admin_personality_path(personality) %></td>
        <td><%= link_to 'Destroy', personality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personality', new_admin_personality_path %>
